@PACKAGE_INIT@

# Include the exported targets
include("${CMAKE_CURRENT_LIST_DIR}/slogTargets.cmake")

# Check required components
check_required_components(slog)

# Set variables for backward compatibility
set(SLOG_VERSION @PROJECT_VERSION@)
set(SLOG_INCLUDE_DIRS "${PACKAGE_PREFIX_DIR}/include")
set(SLOG_LIBRARIES slog::slog_static)

# Provide find_package compatibility
if(NOT TARGET slog::slog)
    add_library(slog::slog ALIAS slog::slog_static)
endif()

